<header>회원가입</header>
<form class="form-horizontal registerForm" action="/signupTest" method="POST">

  <div class="form-group">
      <label for="inputId3" class="col-xs-10 col-sm-2 col-md-3 control-label">아이디</label>
      <div class="col-xs-9 col-sm-7 col-md-4">
        <input type="email" class="form-control" id="inputEmail3" name="email" placeholder="아이디">
      </div>
  </div>

  <div class="form-group">
      <label for="fieldPassword" class="col-xs-10 col-sm-2 col-md-3 control-label">비밀번호</label>
      <div class="col-xs-9 col-sm-7 col-md-4">
        <input type="password" class="form-control" id="fieldPassword" name="password" placeholder="비밀번호">
      </div>
  </div>
  
  <div>
      <label for="fieldPasswordConf" class="col-xs-10 col-sm-2 col-md-3 control-label">비밀번호확인</label>
      <div class="col-xs-9 col-sm-7 col-md-4">
        <input type="password" class="form-control" id="fieldPasswordConf" name="passwordConf" placeholder="비밀번호확인">
      </div>
  </div>

  <button id="register">회원가입</button>
  <button class="goBack">취소</button>
</form>

<a id="kakao-login-btn"></a>
<a href="http://developers.kakao.com/logout"></a>


{{#section 'head'}}
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta name="viewport" content="user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, width=device-width"/>
    <script src="//developers.kakao.com/sdk/js/kakao.min.js"></script>
{{/section}}


{{#section 'jquery'}}
    <script type="text/javascript" src="/js/controllers/registerController.js"></script>
{{/section}}